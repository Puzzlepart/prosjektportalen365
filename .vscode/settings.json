{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/lib": true,
        "**/dist": true,
        "**/temp": true,
        "**/package-lock.json": true,
        "**/pnpm-lock.yaml": true,
        "README.md": true,
        "dev/README.md": true,
        ".husky": true,
    },
    "files.associations": {
        "*.txt": "json",
        ".README": "markdown",
        "dev/.README": "markdown"
    },
}
{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Debug",
      "type": "chrome",
      "request": "launch",
      "url": "https://puzzlepart.sharepoint.com/sites/pp365_channels_test/SitePages/Home.aspx?debug=true&noredir=true&debugManifestsFile=https%3A%2F%2Flocalhost%3A4321%2Ftemp%2Fmanifests.js",
      "webRoot": "${workspaceRoot}/SharePointFramework/PortfolioWebParts",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9222"
      ]
    },
    {
      "name": "Debug in incognito mode",
      "type": "chrome",
      "request": "launch",
      "url": "https://puzzlepart.sharepoint.com/sites/pp365_channels_test/SitePages/Home.aspx?debug=true&noredir=true&debugManifestsFile=https%3A%2F%2Flocalhost%3A4321%2Ftemp%2Fmanifests.js",
      "webRoot": "${workspaceRoot}/SharePointFramework/PortfolioWebParts",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9222",
        "--incognito"
      ]
    }
  ]
}

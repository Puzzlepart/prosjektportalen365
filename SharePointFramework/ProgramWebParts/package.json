  "scripts": {
    "watch": "concurrently \"npm run serve\" \"livereload './dist/*.js' -e 'js' -w 250\"",
    "serve": "concurrently \"gulp serve-deprecated --locale=nb-no --nobrowser\"",
    "package": "npm run build && gulp package-solution --ship",
    "package:debug": "gulp clean && gulp default && gulp package-solution",
    "build": "gulp clean --ship && gulp default --ship",
    "postversion": "tsc && npm publish",
    "lint": "eslint --ext .ts,.tsx ./src --color",
    "lint:fix": "eslint --ext .ts,.tsx ./src --color --fix && npm run prettier",
    "prettier": "prettier --config .prettierrc '**/*.ts*' --write --loglevel silent",
    "publish:temp": "npm install && npm run build && npm publish --tag temp"
  },

{
    "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
    "version": "2.0",
    "bundles": {
        "program-administration-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programAdministration/index.js",
                "manifest": "./src/webparts/programAdministration/manifest.json"
            }]
        },
        "program-project-overview-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programProjectOverview/index.js",
                "manifest": "./src/webparts/programProjectOverview/manifest.json"
            }]
        },
        "program-aggregation-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programAggregation/index.js",
                "manifest": "./src/webparts/programAggregation/manifest.json"
            }]
        },
        "program-timeline-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programTimeline/index.js",
                "manifest": "./src/webparts/programTimeline/manifest.json"
            }]
        }

    },
    "externals": {},
    "localizedResources": {
        "SharedResources": "lib/loc/shared/{locale}.js",
        "ProgramWebPartsStrings": "lib/loc/{locale}.js",
        "PortfolioWebPartsStrings": "lib/loc/{locale}.js",
        "ProjectWebPartsStrings": "node_modules/pp365-projectwebparts/lib/loc/{locale}.js",
        "PzlSPFxComponentsStrings": "node_modules/pp365-projectwebparts/lib/loc/{locale}.js",
        "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
        "ControlStrings": "lib/loc/{locale}.js",
        "SharedLibraryStrings": "node_modules/pp365-shared-library/lib/loc/{locale}.js"
    }
}

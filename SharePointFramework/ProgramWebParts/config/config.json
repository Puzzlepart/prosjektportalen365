{
    "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
    "version": "2.0",
    "bundles": {
        "program-administration-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programAdministration/ProgramAdministrationWebpart.js",
                "manifest": "./src/webparts/programAdministration/ProgramAdministrationWebpart.manifest.json"
            }]
        },
        "program-project-overview-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programProjectOverview/ProgramProjectOverviewWebPart.js",
                "manifest": "./src/webparts/programProjectOverview/ProgramProjectOverviewWebPart.manifest.json"
            }]
        },
        "program-benefits-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programBenefits/ProgramBenefitsWebPart.js",
                "manifest": "./src/webparts/programBenefits/ProgramBenefitsWebPart.manifest.json"
            }]
        },
        "program-deliveries-web-part": {
            "components": [{
                "entrypoint": "./lib/webparts/programDeliveries/ProgramDeliveriesWebPart.js",
                "manifest": "./src/webparts/programDeliveries/ProgramDeliveriesWebPart.manifest.json"
            }]
        },
        "program-timeline-web-part": {
          "components": [
            {
              "entrypoint": "./lib/webparts/programTimeline/ProgramTimelineWebPart.js",
              "manifest": "./src/webparts/programTimeline/ProgramTimelineWebPart.manifest.json"

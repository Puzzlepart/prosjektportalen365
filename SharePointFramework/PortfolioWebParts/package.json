        "@pnp/odata": "1.3.8",
        "@pnp/polyfill-ie11": "1.0.2",
        "@pnp/sp": "1.3.8",
        "@pnp/sp-clientsvc": "1.3.8",
        "@pnp/sp-taxonomy": "1.3.8",
        "@pnp/spfx-controls-react": "1.11.0",
        "array-sort": "1.0.0",
        "clean-deep": "3.0.2",
        "interactjs": "1.6.2",
        "jsom-ctx": "1.2.0",
        "moment": "2.24.0",
        "msgraph-helper": "0.7.3",
        "object-assign": "4.1.1",
        "object-get": "2.1.0",
        "office-ui-fabric-react": "6.214.0",
        "pp365-shared": "1.2.6",
        "pp365-projectwebparts": "1.2.6",
        "react": "16.8.5",
        "react-calendar-timeline": "0.26.6",

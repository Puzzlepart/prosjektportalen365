{
    "name": "pp365-portfoliowebparts",
    "version": "1.2.5",
    "engines": {
        "node": ">=8.0.0 <11.0.0"
    },
    "scripts": {
        "serve": "gulp serve --locale=nb-no --nobrowser",
        "package": "gulp clean --ship && gulp default --ship && gulp package-solution --ship",
        "package:ci": "gulp clean && gulp default && gulp package-solution",
        "deploy": "gulp uploadAppPkg && gulp deploySppkg",
        "packageAndDeploy": "npm run package && npm run deploy",
        "postversion": "gulp versionSync",
        "prepackage": "gulp setHiddenToolbox --ship",
        "preserve": "gulp setHiddenToolbox",
        "preserveNoBrowser": "gulp setHiddenToolbox",
        "lint": "eslint --ext .ts,.tsx ./src --color && npm run prettier",
        "lint:fix": "eslint --ext .ts,.tsx ./src --color --fix && npm run prettier:write",
        "prettier": "prettier --config .prettierrc '**/*.ts*' --loglevel silent",
        "prettier:write": "prettier --config .prettierrc '**/*.ts*' --write --loglevel silent"
    },
    "dependencies": {
        "@microsoft/sp-core-library": "1.11.0",

{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "project-status-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectStatus/ProjectStatusWebPart.js",
          "manifest": "./src/webparts/projectStatus/ProjectStatusWebPart.manifest.json"
        }
      ]
    },
    "project-phases-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectPhases/ProjectPhasesWebPart.js",
          "manifest": "./src/webparts/projectPhases/ProjectPhasesWebPart.manifest.json"
        }
      ]
    },
    "project-information-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectInformation/ProjectInformationWebPart.js",
          "manifest": "./src/webparts/projectInformation/ProjectInformationWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ProjectStatusWebPartStrings": "lib/webparts/projectStatus/loc/{locale}.js",
    "ProjectPhasesWebPartStrings": "lib/webparts/projectPhases/loc/{locale}.js",
    "ProjectInformationWebPartStrings": "lib/webparts/projectInformation/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}

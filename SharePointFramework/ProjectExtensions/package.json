{
  "name": "pp365-projectextensions",
  "version": "1.6.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/Puzzlepart/prosjektportalen365.git",
    "directory": "SharePointFramework/ProjectExtensions"
  },
  "license": "MIT",
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "watch": "concurrently \"npm run serve\" \"livereload './dist/*.js' -e 'js' -w 250\"",
    "serve": "gulp serve-deprecated --locale=nb-no",
    "serve:nobrowser": "gulp serve-deprecated --nobrowser --locale=nb-no",
    "serve:helpcontent": "gulp serve-deprecated --config=HelpContent --locale=nb-no",
    "package": "gulp clean && gulp default --ship && gulp package-solution --ship",
    "package:ci": "gulp clean && gulp default && gulp package-solution",
    "build": "tsc --build",
    "postversion": "tsc && npm publish",
    "lint": "eslint --ext .ts,.tsx ./src --color",
    "lint:fix": "eslint --ext .ts,.tsx ./src --color --fix && npm run prettier",
    "prettier": "prettier --config .prettierrc '**/*.ts*' --write --loglevel silent",
    "publish:temp": "npm install && npm run build && npm publish --tag temp"
  },
  "dependencies": {
    "@microsoft/sp-core-library": "1.15.2",
    "@microsoft/decorators": "1.15.2",
    "@microsoft/sp-dialog": "1.15.2",
    "@microsoft/sp-application-base": "1.15.2",
    "@microsoft/sp-office-ui-fabric-core": "1.15.2",
    "@pnp/common": "1.3.8",
    "@pnp/logging": "1.3.8",
    "@pnp/odata": "1.3.8",
    "@pnp/polyfill-ie11": "1.0.1",
    "@pnp/sp": "1.3.8",
    "@pnp/sp-clientsvc": "1.3.8",
    "@pnp/sp-taxonomy": "1.3.8",
    "@reduxjs/toolkit": "1.5.0",
    "@uifabric/file-type-icons": "7.6.27",
    "@uifabric/utilities": "6.45.1",
    "msgraph-helper": "0.8.3",
    "office-ui-fabric-react": "7.185.7",
    "pp365-shared": "1.6.3-817.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "^8.0.3",
    "rehype-raw": "^6.1.1",
    "sp-entityportal-service": "1.3.3",
    "sp-hubsite-service": "0.8.0",
    "sp-js-provisioning": "1.1.2",
    "spfx-jsom": "0.6.6",
    "tslib": "2.3.1",
    "underscore": "1.9.1",
    "valid-filename": "3.1.0"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-4.5": "0.2.2",
    "@microsoft/eslint-plugin-spfx": "1.15.2",
    "@microsoft/eslint-config-spfx": "1.15.2",
    "@microsoft/sp-build-web": "1.15.2",
    "@rushstack/eslint-config": "2.5.1",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/sharepoint": "2016.1.7",
    "@types/underscore": "1.9.2",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "ajv": "6.12.5",
    "colors": "1.4.0",
    "concurrently": "5.2.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "find": "0.3.0",
    "gulp": "4.0.2",
    "livereload": "0.9.1",
    "prettier": "2.1.1",
    "typescript": "4.5.5",
    "webpack": "^4.44.2"
  }
}

{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "project-setup-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/projectSetup/index.js",
          "manifest": "./src/projectSetup/manifest.json"
        }
      ]
    },
    "project-upgrade-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/projectUpgrade/index.js",
          "manifest": "./src/projectUpgrade/manifest.json"
        }
      ]
    },
    "template-selector-command-set": {
      "components": [
        {
          "entrypoint": "./lib/templateSelector/index.js",
          "manifest": "./src/templateSelector/manifest.json"
        }
      ]
    },
    "risk-action-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/riskAction/index.js",
          "manifest": "./src/riskAction/manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ProjectExtensionsStrings": "lib/loc/{locale}.js",
    "ControlStrings": "lib/loc/{locale}.js",
    "SharedLibraryStrings": "node_modules/pp365-shared-library/lib/loc/{locale}.js"
  }
}

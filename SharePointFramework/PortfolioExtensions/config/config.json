{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "idea-projectdata-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/ideaProjectData/index.js",
          "manifest": "./src/extensions/ideaProjectData/manifest.json"
        }
      ]
    },
    "idea-processing-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/ideaProcessing/index.js",
          "manifest": "./src/extensions/ideaProcessing/manifest.json"
        }
      ]
    },
    "idea-registration-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/ideaRegistration/index.js",
          "manifest": "./src/extensions/ideaRegistration/manifest.json"
        }
      ]
    },
    "footer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/footer/index.js",
          "manifest": "./src/extensions/footer/manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SharedResources": "lib/loc/shared/{locale}.js",
    "PortfolioExtensionsStrings": "lib/loc/{locale}.js",
    "ControlStrings": "lib/loc/{locale}.js",
    "SharedLibraryStrings": "node_modules/pp365-shared-library/lib/loc/{locale}.js"
  }
}

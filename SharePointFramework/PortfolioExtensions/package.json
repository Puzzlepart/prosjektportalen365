{
  "name": "pp365-portfolioextensions",
  "version": "1.6.1",
  "engines": {
    "node": "^12"
  },
  "main": "lib/index.js",
  "scripts": {
    "watch": "concurrently \"npm run serve\" \"livereload './dist/*.js' -e 'js' -w 250\"",
    "serve:nobrowser": "gulp serve-deprecated --nobrowser --locale=nb-no",
    "serve": "gulp serve-deprecated --locale=nb-no",
    "package": "gulp clean && gulp default --ship && gulp package-solution --ship",
    "package:ci": "gulp clean && gulp default && gulp package-solution",
    "deploy": "gulp uploadAppPkg && gulp deploySppkg",
    "packageAndDeploy": "npm run package && npm run deploy",
    "lint": "eslint --ext .ts,.tsx ./src --color",
    "lint:fix": "eslint --ext .ts,.tsx ./src --color --fix && npm run prettier",
    "prettier": "prettier --config .prettierrc '**/*.ts*' --write --loglevel silent"
  },
  "dependencies": {
    "@fluentui/react": "8.96.0",
    "@microsoft/decorators": "1.15.2",
    "@microsoft/sp-core-library": "1.15.2",
    "@microsoft/sp-dialog": "1.15.2",
    "@microsoft/sp-listview-extensibility": "1.15.2",
    "@pnp/core": "^3.7.0",
    "@pnp/logging": "3.7.0",
    "@pnp/sp": "3.7.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "pp365-shared": "temp",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@microsoft/eslint-config-spfx": "1.15.2",
    "@microsoft/eslint-plugin-spfx": "1.15.2",
    "@microsoft/rush-stack-compiler-4.5": "0.2.2",
    "@microsoft/sp-build-web": "1.15.2",
    "@microsoft/sp-module-interfaces": "1.15.2",
    "@microsoft/sp-webpart-workbench": "1.12.1",
    "@rushstack/eslint-config": "2.5.1",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/sharepoint": "^2016.1.10",
    "@types/underscore": "^1.11.4",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "ajv": "6.12.5",
    "concurrently": "7.4.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "gulp": "4.0.2",
    "prettier": "2.7.1",
    "typescript": "4.5.5",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpackbar": "^5.0.2"
  }
}

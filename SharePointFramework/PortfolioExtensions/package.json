{
  "name": "pp365-portfolioextensions",
  "version": "1.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Puzzlepart/prosjektportalen365.git",
    "directory": "SharePointFramework/PortfolioExtensions"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "watch": "concurrently \"npm run serve\" \"livereload './dist/*.js' -e 'js' -w 250\"",
    "serve": "concurrently \"gulp serve-deprecated --locale=nb-no --nobrowser\"",
    "build": "gulp bundle --ship && gulp package-solution --ship",
    "postversion": "tsc && npm publish",
    "lint": "eslint --ext .ts,.tsx ./src --color --fix && npm run prettier",
    "prettier": "prettier '**/*.ts*' --write --loglevel silent",
    "prewatch": "node ../.tasks/pre-watch.js",
    "postwatch": "node ../.tasks/post-watch.js"
  },
  "dependencies": {
    "@fluentui/react": "8.98.1",
    "@microsoft/decorators": "1.15.2",
    "@microsoft/sp-core-library": "1.15.2",
    "@microsoft/sp-dialog": "1.15.2",
    "@microsoft/sp-page-context": "1.15.2",
    "@microsoft/sp-application-base": "1.15.2",
    "@microsoft/sp-listview-extensibility": "1.15.2",
    "react-markdown": "^8.0.3",
    "rehype-raw": "^6.1.1",
    "@pnp/core": "3.17.0",
    "@pnp/logging": "3.17.0",
    "@pnp/sp": "3.17.0",
    "pp365-shared-library": "1.9.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tslib": "2.3.1",
    "underscore": "~1.13.6",
    "@pnp/queryable": "3.17.0"
  },
  "devDependencies": {
    "@microsoft/eslint-config-spfx": "1.15.2",
    "@microsoft/eslint-plugin-spfx": "1.15.2",
    "@microsoft/rush-stack-compiler-4.5": "0.2.2",
    "@microsoft/sp-build-web": "1.15.2",
    "@microsoft/sp-module-interfaces": "1.15.2",
    "@rushstack/eslint-config": "2.5.1",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "@types/sharepoint": "2016.1.10",
    "@types/underscore": "~1.11.5",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "ajv": "6.12.5",
    "concurrently": "7.4.0",
    "dotenv": "~16.1.3",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-unused-imports": "2.0.0",
    "gulp": "4.0.2",
    "livereload": "0.9.3",
    "prettier": "2.7.1",
    "typescript": "4.5.5",
    "webpack": "5.74.0",
    "@types/lodash": "~4.14.195"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "prettier",
      "unused-imports"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "@typescript-eslint/interface-name-prefix": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-inferrable-types": 0,
      "react/prop-types": 0,
      "no-inferrable-types": 0,
      "react/display-name": 0,
      "no-compare-neg-zero": 1,
      "no-console": 2,
      "default-case": 0,
      "eqeqeq": 1,
      "max-classes-per-file": 1,
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "yoda": 2,
      "require-await": 1,
      "semi": [
        "error",
        "never",
        {}
      ],
      "unused-imports/no-unused-imports": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false,
    "arrowParens": "always",
    "printWidth": 100,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true
  }
}
